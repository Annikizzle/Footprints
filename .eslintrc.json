{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readable",
        "SharedArrayBuffer": "readable"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "import/newline-after-import": "error",
        "new-cap": 0,
        "no-caller": 2,
        "no-undef": 2,
        "no-unused-vars": ["error", { "args": "none" }],
        "no-empty": ["error", { "allowEmptyCatch": true }],
        "no-console": "off",
        "no-plusplus":"off",
        "max-len":"off",
        "no-use-before-define":"off",
        "vars-on-top":"off",
        "no-var":"off",
        "block-scoped-var":"off",
        "no-redeclare":"off",
        "radix":"off",
        "no-restricted-globals":"off",
        "func-names":"off",
        "import/no-extraneous-dependencies":"off"

    }
}